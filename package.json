{
  "name": "metric_dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "app/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:electron\"",
    "dev:next": "next dev --turbo",
    "dev:electron": "wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 electron .",
    "build": "next build --turbopack",
    "start": "electron .",
    "package": "electron-packager . metric_dashboard --platform=darwin --arch=x64 --out=dist"
  },
